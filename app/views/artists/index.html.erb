<h1> Artists </h1>


<ul>
<% @artists.each do |a| %>
    <li>
        <h3><%= link_to a.name, artist_path(a) %></h3>
        <h4><%= a.song_count %> song</h4>
        <ul>
            <% a.songs.each do |s| %>
            <li><%= link_to s.title, song_path(s) %></li>
            <% end %>
        </ul>
    </li>
<% end %>
</ul>